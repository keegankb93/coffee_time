<!DOCTYPE html>
<html lang="en" class="bg-gray-950">
<%= render "layouts/main" do |partial| %>

  <% partial.head do %>
    <%= render "shared/head" %>
  <% end %>

  <% partial.nav do %>
    <%= render "shared/nav" %>
  <% end %>

  <% partial.header content_for(:page_title) %>

  <% partial.main do %>
    <%= yield %>
  <% end %>

<% end %>
</html>
